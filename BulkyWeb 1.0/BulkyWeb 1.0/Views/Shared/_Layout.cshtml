<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Cotiviti Parichya</title>
    <link rel="shortcut icon" type="image/x-icon" href="~/favicon.png">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@3.3.7/dist/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/css/toastr.min.css" />
    
</head>
<body>
    <nav class="navbar navbar-default">
        <div class="container-fluid">
            <div class="navbar-header">
                <a class="navbar-brand" href="#">Cotiviti Parichya</a>
            </div>
            <ul class="nav navbar-nav">
                <li><a href="#">Home</a></li>
                <li><a href="/Dashboard/Display">Dashboard</a></li>
                @if (User.IsInRole("admin"))
                {
                    <li><a href="/Admin/Display">Admin Page</a></li>
                    <li><a href="/Quiz/Index">QuizList</a></li>
                    <li><a href="/Quiz/Report">Results</a></li>

                }
                <li><a href="/Quiz/QA">Quiz (Doesn't Work)</a></li>
                <li><a href="/Quiz/Q">PLAY!</a></li>

                @if (!string.IsNullOrEmpty(User.Identity.Name))
                {
                    <li><a href="/UserAuthentication/ChangePassword">ChangePassword</a></li>
                    <li><a href="/UserAuthentication/Logout">Logout</a></li>
                }
            </ul>
        </div>
    </nav>


    <div class="container">
        <partial name="_notification" />
        @RenderBody()
    </div>

    <footer class="border-top footer text-muted" style="
  bottom: 0;
  width: 100%;
  height: 50px;
  background-color: #f5f5f5;
  border-top: 1px solid #ddd;
  text-align: center;
  padding-top: 15px;">
        <div class="text-center">
            &copy; 2023 - BulkyWeb_1._0 - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>